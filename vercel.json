{
  "buildCommand": "mkdir -p public && cp -r src/* public/",
  "devCommand": "npx http-server src -p 8000",
  "installCommand": "npm install",
  "outputDirectory": "public",
  "framework": null,
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        }
      ]
    }
  ]
}
